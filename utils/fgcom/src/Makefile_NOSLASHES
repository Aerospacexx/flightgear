##
## Makefile for the FGCom project.
##





ifneq (,$(findstring Linux,$(shell uname)))
  OSTYPE=LINUX
else
  ifneq (,$(findstring Darwin,$(shell uname)))
    OSTYPE=MACOSX
  else
    # CYGWIN reports CYGWIN_NT-5.0 under Win2K
    ifneq (,$(findstring WIN,$(shell uname)))
      OSTYPE=WIN32
    else
      ifneq (,$(findstring MINGW,$(shell uname)))
        OSTYPE=WIN32
      else
        ifneq (,$(findstring SunOS,$(shell uname)))
          OSTYPE=SOLARIS
        else
          $(warning OSTYPE cannot be detected, assuming Linux)
          OSTYPE=LINUX
        endif
      endif
    endif
  endif
endif

ifeq ($(OSTYPE),LINUX)
PLIB_PREFIX := MY_SLASH_HEREhomeMY_SLASH_HEREclementMY_SLASH_HEREfgfsMY_SLASH_HEREinstallMY_SLASH_HEREplib
OPENAL_PREFIX := MY_SLASH_HEREusrMY_SLASH_HERElocalMY_SLASH_HEREsrcMY_SLASH_HEREfgfs-builderMY_SLASH_HEREinstall
SVNDEF := -D'SVN_REV="$(shell svnversion -n .)"'
INSTALL_BIN := MY_SLASH_HEREhomeMY_SLASH_HEREclementMY_SLASH_HEREfgfsMY_SLASH_HEREinstallMY_SLASH_HEREfgcomMY_SLASH_HEREbin
INSTALL_DIR := MY_SLASH_HEREhomeMY_SLASH_HEREclementMY_SLASH_HEREfgfsMY_SLASH_HEREinstallMY_SLASH_HEREfgcomMY_SLASH_HEREfgcom
SIMGEAR_INC := ~MY_SLASH_HEREtmpMY_SLASH_HEREsimgear
DEBUG := -O0 -Wall -ggdb
