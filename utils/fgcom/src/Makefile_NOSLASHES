STATIC_LIBS:=..MY_SLASH_HEREiaxclientMY_SLASH_HERElibMY_SLASH_HERElibiaxclient.a -lpthread -lm
INDENT:=MY_SLASH_HEREusrMY_SLASH_HEREbinMY_SLASH_HEREindent
IFLAGS:=-DDEFAULT_POSITIONS_FILE=\"$(INSTALL_DIR)MY_SLASH_HEREpositions.txt\"
INSTALL_BIN:=..MY_SLASH_HEREFlightGearOSXMY_SLASH_HEREbuildMY_SLASH_HEREDeploymentMY_SLASH_HEREFlightGear.appMY_SLASH_HEREContentsMY_SLASH_HEREResources
INSTALL_DIR:=$(INSTALL_BIN)MY_SLASH_HEREdataMY_SLASH_HEREfgcom
endif

##
## Look for all C source files (.c & .cpp)
##
CPPFILES := $(wildcard *.cpp)
HFILES := $(patsubst %.cpp,%.h,$(CPPFILES))
OFILES := $(patsubst %.cpp,%.o,$(CPPFILES))



.PHONY:	all iaxclient fgcomstatic indent install clean list


all: fgcomstatic iaxclient

iaxclient:
	 cd ..MY_SLASH_HEREiaxclientMY_SLASH_HERElib && make

fgcomstatic: $(OFILES)
	cd ..MY_SLASH_HEREiaxclientMY_SLASH_HERElib && make
	$(CXX) $(LDFLAGS) $(OFILES) $(STATIC_LIBS) -o fgcom

indent:	$(CPPFILES) $(HFILES)
	$(INDENT) $(IFLAGS) $(CPPFILES) $(HFILES)

install: all
	install -m 755 -s fgcom $(INSTALL_BIN)MY_SLASH_HEREfgcom
	install -d -m 755 $(INSTALL_DIR)
	install -m 644 ..MY_SLASH_HEREdataMY_SLASH_HEREpositions.txt $(INSTALL_DIR)
	install -m 644 ..MY_SLASH_HEREdataMY_SLASH_HEREphonebook.txt $(INSTALL_DIR)
	install -m 644 ..MY_SLASH_HEREdataMY_SLASH_HEREspecial_frequencies.txt $(INSTALL_DIR)

clean:
	cd ..MY_SLASH_HEREiaxclientMY_SLASH_HERElib && make clean
	rm -f *.o fgcom *~

%o:	%cpp $(HFILES)
	$(CXX) $(CXXFLAGS) -c -o $@ $<

list:	
	@$(ECHO) "Source files: \t" $(CPPFILES)
	@$(ECHO) "Header files: \t" $(HFILES)
	@$(ECHO) "Object files: \t" $(OFILES)

#
# End of file.
